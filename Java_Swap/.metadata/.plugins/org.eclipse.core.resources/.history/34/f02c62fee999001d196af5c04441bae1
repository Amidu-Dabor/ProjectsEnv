package SwapStrings;
import java.util.*;

public class Swap {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		SwapConfig swapObj = new SwapConfig();
		Scanner strInput = new Scanner(System.in);
		
		System.out.println("Enter your first name: ");
		String firstName = strInput.nextLine();
		System.out.println("Enter your last name: ");
		String lastName = strInput.nextLine();
		
		System.out.println("-------------------- NAMES BEFORE SWAPPING --------------------");
		swapObj.setNames(firstName, lastName);
		swapObj.getNames();
		
		System.out.println("-------------------- NAMES AFTER SWAPPING --------------------" + "\n");
		swapObj.swapNames();
		
	}

}
